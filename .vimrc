syntax on
filetype plugin indent on
" show existing tab with 4 spaces width
set tabstop=4
" when indenting with '>', use 4 spaces width
set shiftwidth=4
" On pressing tab, insert 4 spaces
set expandtab
set cc=80
"set t_Co=256
" set background=dark
"colorscheme luna
"highlight Normal ctermbg=NONE
"highlight nonText ctermbg=NONE
set visualbell
